<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ashish.columnandrow">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ColumnAndRow">
        <activity
            android:name=".NetConstraintLayout"
            android:exported="true"
            android:label="@string/title_activity_net_constrinat_layout"
            android:theme="@style/Theme.ColumnAndRow.NoActionBar" />
        <activity
            android:name=".CustomCheckBox"
            android:exported="true"
            android:label="@string/checkbox"
            android:theme="@style/Theme.ColumnAndRow.NoActionBar" />
        <activity
            android:name=".CircularProgressBar"
            android:exported="true"
            android:label="@string/title_activity_circular_progress_bar"
            android:theme="@style/Theme.ColumnAndRow.NoActionBar" />
        <activity
            android:name=".ConstraintLayoutCompose"
            android:exported="true"
            android:label="@string/title_activity_constraint_layout_compose"
            android:theme="@style/Theme.ColumnAndRow.NoActionBar" />
        <activity
            android:name=".List"
            android:exported="true"
            android:label="@string/title_activity_list"
            android:theme="@style/Theme.ColumnAndRow.NoActionBar" />
        <activity
            android:name=".StateDemo"
            android:exported="true"
            android:label="@string/title_activity_state_demo"
            android:theme="@style/Theme.ColumnAndRow.NoActionBar" />
        <activity
            android:name=".TextViewCompose"
            android:exported="true"
            android:label="@string/button_textview"
            android:theme="@style/Theme.ColumnAndRow.NoActionBar" />
        <activity
            android:name=".ImageCard"
            android:exported="true"
            android:label="@string/title_activity_image_card"
            android:theme="@style/Theme.ColumnAndRow.NoActionBar" />
        <activity
            android:name=".HomeActivity"
            android:exported="true"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.ColumnAndRow.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RowAndColumn"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ColumnAndRow.NoActionBar" />
    </application>

</manifest>